package report;

public class Report {

	public static void main(String[] args) {
		int kor = 0;
		int eng = 0;
		int math = 0;
		int sci = 0;
		System.out.println(kor + eng + math + sci);
		System.out.println(kor = 90);
		System.out.println(eng = 80);
		System.out.println(math = 70);
		System.out.println(sci = 60);
		
		
			
		
				

	}

}
